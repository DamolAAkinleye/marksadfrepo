{
	"name": "pipeline11",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": "*.csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['InvoiceId']"
								},
								"sink": {
									"name": "InvoiceId"
								}
							},
							{
								"source": {
									"path": "$['PayorOrgId']"
								},
								"sink": {
									"name": "PayorOrgId"
								}
							},
							{
								"source": {
									"path": "$['Status']"
								},
								"sink": {
									"name": "Status"
								}
							},
							{
								"source": {
									"path": "$['StatusNote']"
								},
								"sink": {
									"name": "StatusNote"
								}
							},
							{
								"source": {
									"path": "$['StatusDate']"
								},
								"sink": {
									"name": "StatusDate"
								}
							},
							{
								"source": {
									"path": "$['PayorOrgEBillingId']"
								},
								"sink": {
									"name": "PayorOrgEBillingId"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Json5",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "folderout",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"myvar": {
				"type": "String"
			}
		},
		"annotations": []
	}
}